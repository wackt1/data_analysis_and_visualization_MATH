  /VARIABLES=Q7.3_1 Q7.4_1
  /SCALE('ALL VARIABLES') ALL
  /MODEL=ALPHA
  /SUMMARY=TOTAL.

COMPUTE Voraussetzungsbewusstsein=MEAN(Q7.3_1,Q7.4_1).
